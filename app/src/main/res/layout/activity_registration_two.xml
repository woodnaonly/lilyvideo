                    <EditText android:textAppearance="?android:textAppearanceMedium" android:textSize="@dimen/sp_14"
                              android:textColor="@color/color_323232" android:textColorHint="@color/color_DBDBDB"
                              android:id="@+id/register_phone_p" android:background="@null"
                              android:layout_width="wrap_content" android:layout_height="wrap_content"
                              android:maxLength="11"
                              android:layout_marginLeft="@dimen/dp_25" android:layout_marginRight="@dimen/dp_5"
                              android:hint="请输入手机号" android:singleLine="true" android:inputType="number"/>
                </LinearLayout>
                <View android:id="@+id/view_line02_02" android:background="@color/color_F3F3F3"
