                  android:layout_marginRight="@dimen/dp_48" android:layout_marginBottom="@dimen/dp_10"
                  android:text="注册"/>
    </LinearLayout>
    <RelativeLayout android:orientation="vertical" android:id="@+id/linelayout_regist_phone_one"
